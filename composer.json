{
    "name": "blitz-php/translator",
    "type": "library",
    "description": "Module de traduction de Blitz PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri Sitchet Tomkeu",
            "email": "devcode.dst@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
	    "blitz-php/autoloader": "^1"
    },
    "autoload": {
        "psr-4": {
            "BlitzPHP\\Translator\\": ""
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "support": {
        "issues": "https://github.com/blitz-php/framework/issues",
        "source": "https://github.com/blitz-php/framework"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
